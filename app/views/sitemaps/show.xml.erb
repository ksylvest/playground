<%= render 'entry', path: root_url, frequency: 'weekly' %>
<%= render 'entry', path: login_url, frequency: 'weekly' %>
<%= render 'entry', path: signup_url, frequency: 'weekly' %>

<%- @users.each do |user| -%>
<%= render 'entry', path: profile_url(user), frequency: 'monthly' %>
<%- end -%>

<%- @feed.entries.each do |entry| -%>
<%= render 'entry', path: feed_entry_url(entry), frequency: 'monthly' %>
<%- end %>
